// C program to swap two variables 

#include <stdio.h> 

int main() 
{ 

     int x = 10, y = 20; 

     printf("Enter Value of x "); 

     scanf("%d", &x); 

     printf("\nEnter Value of y "); 

     scanf("%d", &y); 
     
     printf("\nBefore Swapping: x = %d, y = %d", x, y);

     int temp = x ;
     
     x = y ;

     y = temp ;
 
     printf("\nAfter Swapping: x = %d, y = %d", x, y); 

    return 0; 

} 
